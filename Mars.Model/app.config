<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IPromotionManager" />
                <binding name="BasicHttpBinding_IConferenceManager" />
                <binding name="BasicHttpBinding_ILinkManager" />
                <binding name="BasicHttpBinding_IMemberManager" />
                <binding name="BasicHttpBinding_INewsManager" />
                <binding name="BasicHttpBinding_IProjectManager" />
                <binding name="BasicHttpBinding_ISlideManager" />
                <binding name="BasicHttpBinding_IShowManager" />
                <binding name="BasicHttpBinding_ICampusManager" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://www.epsilab.net/webservices/ManagersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPromotionManager"
                contract="ManagersService.IPromotionManager" name="BasicHttpBinding_IPromotionManager" />
            <endpoint address="http://www.epsilab.net/webservices/ManagersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IConferenceManager"
                contract="ManagersService.IConferenceManager" name="BasicHttpBinding_IConferenceManager" />
            <endpoint address="http://www.epsilab.net/webservices/ManagersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILinkManager"
                contract="ManagersService.ILinkManager" name="BasicHttpBinding_ILinkManager" />
            <endpoint address="http://www.epsilab.net/webservices/ManagersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMemberManager"
